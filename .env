# Application Configuration
APP_NAME=WebGenie Backend
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true

# Server
HOST=0.0.0.0
PORT=8000
RELOAD=true

# Database
DATABASE_URL=sqlite:///./webgenie.db

# Celery/Redis
REDIS_URL=redis://localhost:6379
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:8000","http://localhost:5173"]

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Job Configuration
JOB_TIMEOUT=3600
MAX_CONCURRENT_JOBS=4

# Beeline
BEELINE_TIMEOUT=1800
SUPPORTED_ALGORITHMS=["GRNBoost2","SCENIC","PIDC","CLR","ARACNE","NES","Inferelator","pySCENIC"]

